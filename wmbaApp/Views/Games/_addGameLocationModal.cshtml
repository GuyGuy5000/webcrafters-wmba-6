<!-- Modal-->
<div class="modal fade" id="addGameLocationModal" tabindex="-1" role="dialog" aria-labelledby="GameLocationModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="GameLocationmyModalLabel">Add New Game Location</h4>
                <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body">
                @{
                    var m = new wmbaApp.Models.GameLocation();
                    <partial name="_GameLocationCreate" model="m" />
                }
            </div>
        </div>
    </div>
</div>