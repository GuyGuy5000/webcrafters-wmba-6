@model wmbaApp.Models.Division


<hr />

        <form asp-action="Create" class="mx-auto mt-4 editform" id="createForm" style="width: 80%;">
    <fieldset>
        <partial name="_ViewHeading" />
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group">
            <label asp-for="DivName" class="control-label required-label"></label>
            <input asp-for="DivName" class="form-control" placeholder="Ex: Green Jr. Jackfish" />
            <span asp-validation-for="DivName" class="text-danger"></span>
        </div>
        <div class="form-group">
            <input type="submit" value="Create" class="btn btn-outline-dark " />
            <a asp-action="Index" class="btn btn-outline-dark">Back to Division List</a>
        </div>
    </fieldset>
            
        </form>




@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
