@model wmbaApp.Models.Team

<h2>Are you sure you want to deactivate this team?</h2>
<br />
<div>
    <h4>Team</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.TmName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TmName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Division)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Division.DivName)
        </dd>
    </dl>
    <form asp-action="MakeActive">
        <input type="hidden" asp-for="ID" />
        <input type="submit" value="Reactivate" class="btn btn-danger" />
        <div>
            <partial name="_BackToList" />
        </div>
    </form>
</div>