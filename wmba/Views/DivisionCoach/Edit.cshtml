@model wmbaApp.Models.DivisionCoach



<hr />

<form asp-action="Edit" class="mx-auto mt-4 editform" id="createForm" style="width: 80%;">
    <fieldset>
        <partial name="_ViewHeading" />

        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <input type="hidden" asp-for="DivisionID" />
        <input type="hidden" asp-for="CoachID" />
        <div class="form-group">
            <label asp-for="TeamID" class="control-label"></label>
            <select asp-for="TeamID" class="form-control" asp-items="ViewBag.TeamID"></select>
            <span asp-validation-for="TeamID" class="text-danger"></span>
        </div>
        <div class="form-group">
            <input type="submit" value="Save" class="btn btn-outline-dark " />
        </div>

        <div>
            <a asp-action="Index" class="btn btn-outline-dark">Back to Division-Coach List</a>
        </div>
    </fieldset>
   </form>
   



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
